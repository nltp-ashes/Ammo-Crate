    <!-- Out-of-date trader auto_inject.script -->
    <string id="st_ac_err_outdated_trader_autoinject_script_title">
        <text>Script trader_autoinject.script out-of-date or missing.</text>
    </string>

    <string id="st_ac_err_outdated_trader_autoinject_script_what">
        <text>The script trader_autoinject.script is out of date. Ammo Crate comes with an up-to-date one, meaning one of your addons overwrote it. Please arrange your addons so the latest version of that script gets enabled/added last.</text>
    </string>

    <string id="st_ac_err_outdated_trader_autoinject_script_why">
        <text>Ammo Crate requires this script to be up-to-date to enable certain trade features.</text>
    </string>

    <string id="st_ac_err_outdated_trader_autoinject_script_where">
        <text>You can download the latest version from Github : https://github.com/ahuyn/anomaly-dependencies/tree/master/trader_autoinject</text>
    </string>

    <string id="st_ac_open">
        <text>Open</text>
    </string>

    <!--=============================================================================================================-->
